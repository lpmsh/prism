{
  "name": "api",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir=dist --target=bun",
    "start": "bun run dist/index.js",
    "lint": "eslint --max-warnings 0",
    "check-types": "tsc --noEmit",
    "test": "bun run test/run.ts",
    "test:destroy": "bun run test/run.ts --destroy"
  },
  "dependencies": {
    "@daytonaio/sdk": "^0.138.0",
    "hono": "^4.6.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/bun": "latest",
    "eslint": "^9.39.1",
    "typescript": "5.9.2"
  }
}
